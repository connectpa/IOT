    "start-prod": "NODE_ENV=production nodemon server.js"
  },
  "dependencies": {
    "@azure/service-bus": "^1.1.7",
    "@google-cloud/pubsub": "^1.7.3",
    "amqplib": "^0.5.6",
    "aws-sdk": "^2.677.0",
    "azure-sb": "^0.11.1",
    "config": "^3.3.1",
    "js-yaml": "^3.12.0",
    "kafkajs": "^1.12.0",
    "long": "^4.0.0",
    "uuid-parse": "^1.0.0",
    "uuid-random": "^1.3.0",
    "winston": "^3.0.0",
    "winston-daily-rotate-file": "^3.2.1"
  },
  "engines": {
    "node": ">=8.0.0 <11.0.0"
  },
  "nyc": {
    "exclude": [
      "test",
      "__tests__",
      "node_modules",
      "target"
    ]
  },
  "devDependencies": {
    "fs-extra": "^6.0.1",
    "nodemon": "^1.17.5",
    "pkg": "^4.4.8"
